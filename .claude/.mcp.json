{
  "mcpServers": {
    "apple-reminders": {
      "command": "npx",
      "args": [
        "mcp-proxy",
        "http://host.docker.internal:8787/sse"
      ]
    },
    "notion-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@notionhq/notion-mcp-server"
      ],
      "env": {
        "NOTION_TOKEN": "${NOTION_TOKEN}"
      }
    },
    "mcp-gsuite": {
      "command": "uv",
      "args": [
        "--directory",
        "$CLAUDE_PROJECT_DIR/mcp-gsuite",
        "run",
        "mcp-gsuite-enhanced"
      ],
      "env": {
        "PYTHONPATH": "$CLAUDE_PROJECT_DIR/mcp-gsuite",
        "UV_PROJECT_ENVIRONMENT": "$CLAUDE_PROJECT_DIR/mcp-gsuite/.venv"
      }
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    }
  }
}
